{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "no-shadowed-variable": false,
    "no-empty": false,
    "member-ordering": false,
    "member-access": false,
    "curly": false,
    "import-name": false,
    "ter-arrow-parens": false,
    "variable-name": false,
    "object-shorthand-properties-first": false,
    "prefer-array-literal": false,
    "no-console": false,
    "quotemark": [
      true,
      "double"
    ]
  }
}